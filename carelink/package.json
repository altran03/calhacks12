    "setup": "npm install && cd backend && python3 -m venv venv && source venv/bin/activate && pip install -r requirements.txt"
  },
  "dependencies": {
    "@headlessui/react": "^2.0.0",
    "@tailwindcss/postcss": "^4.1.16",
