{
  "name": "carelink",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "backend": "cd backend && source venv/bin/activate && python main.py",
    "agents": "cd backend && source venv/bin/activate && python agents.py",
